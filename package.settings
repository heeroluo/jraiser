{
	"app_path": "./_examples_/",
	"lib_path": "./",
	"list": [
		{ "target": "^base/.*/(index(.js)?)?$" },
		{ "target": "^dom/.*/(index(.js)?)?$" },
		{
			"target": "^ajax/.*/(index(.js)?)?$",
			"excepts": ["^base/.*"]
		},
		{
			"target": "^event-driven/.*/(index(.js)?)?$",
			"includes": ["^event-driven/"]
		},
		{
			"target": "^widget/.*/(index(.js)?)?$",
			"excepts": ["^base/.*", "^dom/.*"]
		},
		{
			"target": "^slideshow/.*/(index(.js)?)?$",
			"excepts": ["^base/.*", "^dom/.*", "^widget/.*"]
		},
		{
			"target": "^validator/.*/(index(.js)?)?$",
			"excepts": ["^base/.*", "^dom/.*", "^ajax/.*", "^widget/.*"]
		}
	],
	"ignored_paths": ["_examples_", "bowl"]
}